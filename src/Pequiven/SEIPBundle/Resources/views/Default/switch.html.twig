    <style>
        h1 {
            color: #DC2E2E;
        }

        li {
            color: red;
        }

        body li {
            color: red;
            font-weight: bold;
            text-decoration: none;
            font-size: larger;
        }

        body a {
            color: white !important;
        }

        body a:hover {
            color: #607890 !important;
        }
    </style>
    <body ng-controller="MainController" class="clearfix with-menu ng-scope menu-hidden" >
        <h1>SELECCIONE UNA EMPRESA</h1>

        <ul id="navigation">
            {% for company in companies %}
                {% set break = false %} 
                {% set company_link = "#!" %}

                {% for item in available if not break %}
                    {% if item.company == company.id %}
                        {% set company_link = path('switch_server', {'connection': item.server, 'url': lastUrl}) %}
                        {% set break = true %}
                    {% endif %}
                {% endfor %}
                
                <li>
                    <a href="{{ company_link }}">
                        <img style="display:block; width:100px;height:100px;" src="{{ company.base64image }}" />
                        <br/>{{ company.alias }}
                    </a>
                </li>
            {% endfor %}
        </ul>        
    </body>
</html>
{% endspaceless %}