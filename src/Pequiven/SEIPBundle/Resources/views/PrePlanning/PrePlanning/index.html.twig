{% extends "::base.html.twig" %}

{% trans_default_domain 'PequivenSEIPBundle' %}

{% block body %}
    {% set period = app.request.get('period') %}
    {{ contentHeader(
            ("pequiven_seip.pre_planning.pre_planning"|trans({'%period%':period}))
        )
    }}
    
    <div id="form-pre-plannig" style="width: 100%;min-height: 600px"></div>
    
{% endblock body %}

{% block javascript %}
    <script type="text/javascript">
        $(function() {
            $('#form-pre-plannig').html('<iframe id="site" src="{{ path('pequiven_pre_planning_form') }}" style="padding:0;width:100%;height:600px"/>');
        });
    </script>
{% endblock javascript %}