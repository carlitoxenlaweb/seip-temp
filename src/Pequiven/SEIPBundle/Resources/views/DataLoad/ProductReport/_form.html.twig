{% trans_default_domain 'PequivenSEIPBundle' %}

<fieldset class="fieldset fields-list">
    <legend class="legend">{{ 'pequiven_seip.product_report.product_report'|trans }}</legend>
    <div class="field-block button-height">
        {{ form_label(form.productionLine) }}
        {{ form_widget(form.productionLine) }}
        {{ form_errors(form.productionLine) }}
    </div>
    <div class="field-block button-height">
        {{ form_label(form.product) }}
        {{ form_widget(form.product) }}
        {{ form_errors(form.product) }}
    </div>
    <div class="field-block button-height">
        {{ form_label(form.typeProduct) }}
        {{ form_widget(form.typeProduct) }}
        {{ form_errors(form.typeProduct) }}
    </div>
    <div class="field-block button-height">
        {{ form_label(form.productUnit) }}
        {{ form_widget(form.productUnit) }}
        {{ form_errors(form.productUnit) }}
    </div>
    <div class="field-block button-height">
        {{ form_label(form.company) }}
        {{ form_widget(form.company) }}
        {{ form_errors(form.company) }}
    </div>
    <div class="field-block button-height">
        {{ form_label(form.location) }}
        {{ form_widget(form.location) }}
        {{ form_errors(form.location) }}
    </div>
    <div class="field-block button-height">
        {{ form_label(form.plant) }}
        {{ form_widget(form.plant) }}
        {{ form_errors(form.plant) }}
    </div>
    <div class="field-block button-height">
        {{ form_label(form.rawMaterial) }}
        {{ form_widget(form.rawMaterial) }}
        {{ form_errors(form.rawMaterial) }}
    </div>
    <div class="field-block button-height">
        {{ form_label(form.isRawMaterial) }}
        {{ form_widget(form.isRawMaterial) }}
        {{ form_errors(form.isRawMaterial) }}
    </div>
    <div class="field-block button-height">
        {{ form_label(form.isFinalProduct) }}
        {{ form_widget(form.isFinalProduct) }}
        {{ form_errors(form.isFinalProduct) }}
    </div>
    <div class="field-block button-height">
        {{ form_label(form.enabled) }}
        {{ form_widget(form.enabled) }}
        {{ form_errors(form.enabled) }}
    </div>
</fieldset>
